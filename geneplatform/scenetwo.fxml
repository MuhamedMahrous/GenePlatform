<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@gene.css"  xmlns:fx="http://javafx.com/fxml" fx:controller="geneplatform.Scenetwo">
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Nodes">
               <VBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </VBox.margin></Label>
            <ListView fx:id="nodes_list">
               <VBox.margin>
                  <Insets />
               </VBox.margin></ListView>
            <Label text="Relations">
               <VBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </VBox.margin></Label>
            <ListView fx:id="relations_list" />
            <Label text="Container">
               <VBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="container_list" />
         </children>
      </VBox>
   </right>
   <center>
      <VBox fx:id="workspace" BorderPane.alignment="CENTER">
         <children>
         </children></VBox>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <bottom>
      <Button fx:id="finish" mnemonicParsing="false" onAction="#finish" text="Finish" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
