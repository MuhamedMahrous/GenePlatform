<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<BorderPane fx:controller="geneplatform.Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@gene.css" xmlns:fx="http://javafx.com/fxml" >
   <center>
      <VBox id="vbox_scene_one" BorderPane.alignment="CENTER">
         <children>
            <Label text="Add input directories" />
            <HBox id="hb_scene_one" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ListView id="dir_list" fx:id="directories" maxHeight="-Infinity" prefHeight="200.0" prefWidth="200.0" />
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
                     <children>
                        <Button id="add_button" fx:id="add_button" mnemonicParsing="false" onAction="#on_add" text="+" />
                        <Button id="remove_button" fx:id="remove_button" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#on_remove" prefHeight="26.0" prefWidth="29.0" text="-" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <Button fx:id="next_button" mnemonicParsing="false" text="Next " textAlignment="CENTER" BorderPane.alignment="CENTER" onAction="#on_next"/>
   </bottom>
</BorderPane>
